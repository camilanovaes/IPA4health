void setup() 
{
  Serial.begin(115200);
  tft.initR(INITR_BLACKTAB);
  tft.fillScreen(BMO_COLOR);
  tft.setRotation(1);
  tft.setTextSize(2);
  tft.setTextColor(ST7735_BLACK);
  //while(Serial.read() != 'f'){
    tft.setCursor(10, 30);
    tft.println("INITIALIZING");
    tft.setCursor(45, 65);
    tft.println("SYSTEM");
  //}
  delay(20000);
  tft.fillScreen(BMO_COLOR);
}

void loop() 
{
  char buff = Serial.read();
  switch(buff) {
    case 'i':
      Serial.println("o");
    break;
    case 's':
      StartUpdate = handleCalibration();
      if(StartUpdate){
        setupFinalDisplay();
        sensor.resetFifo();
      }else{
        ErrorScreen();
        delay(3000);
      }
      while(Serial.read() != 'f' && StartUpdate) { 
        pox.update();
        if (millis() - tsLastReport > REPORTING_PERIOD_MS) {
          iBPM = pox.getHeartRate();
          Serial.println(iBPM);
          SPO2 = pox.getSpO2();
          if (SPO2 >= 100)
            SPO2 = 100;
          tsLastReport = millis();
          FirstSamples++;
        }
        if(FirstSamples > 8){
          if (((iBPM != iBPMOld) && (iBPM < UPPER_PULSE_RATE)) | (SPO2 != SPO2Old)) {
            if(iBPM >= 100)
              UpdateValues(60, 30, iBPMOld, iBPM, 20);
            else if(iBPM >= 10)
              UpdateValues(70, 30, iBPMOld, iBPM, 20);
            else
              UpdateValues(80, 30, iBPMOld, iBPM, 20);
            if(SPO2 >= 100)  
              UpdateValues(60, 85, SPO2Old, SPO2, 75);
            else if(SPO2 >= 10)  
              UpdateValues(75, 85, SPO2Old, SPO2, 75);
            else  
              UpdateValues(85, 85, SPO2Old, SPO2, 75);
            iBPMOld = iBPM;
            SPO2Old = SPO2;
          }
        }
      }
      FirstSamples = 0;
      pox.shutdown();
      tft.fillScreen(BMO_COLOR);
    break;
    case 'n':
      tft.fillScreen(BMO_COLOR);
      tft.setTextColor(ST7735_BLACK);
      tft.setTextSize(2);
      tft.setCursor(10, 30);
      while(Serial.read() != 'f'){
        tft.println("LOADING CARD");
        tft.setCursor(15, 70);
        tft.println("PLEASE WAIT");
        tft.setCursor(10, 30);  
      }
      tft.fillScreen(BMO_COLOR);
      tft.setCursor(15, 50);
      tft.println("CARD SAVED!");
      delay(2000);
      tft.fillScreen(BMO_COLOR);
    break;
    case 'h':
      tft.fillScreen(BMO_COLOR);
      tft.drawBitmap(0, 0, BMO_Blink, 160, 128, ST7735_BLACK);
      delay(700);
      tft.fillScreen(BMO_COLOR);
    break;
    case 'p':
      tft.fillScreen(BMO_COLOR);
      tft.drawBitmap(0, 0, BMO_Angry, 160, 128, ST7735_BLACK);
      delay(2000);
      tft.setTextColor(ST7735_BLACK);
      tft.setTextSize(3);
      tft.fillScreen(BMO_COLOR);
      tft.setCursor(10, 50);
      tft.println("POWEROFF");
      delay(10000);
      tft.fillScreen(BMO_COLOR);
    break;
    default:
      // read the analog in value:
      outputValue = analogRead(analogInPin);
      if((outputValue>=(MinAudioValue-5) && outputValue<=(MaxAudioValue+5))| FirstUpdate)
        tft.drawBitmap(0, 0, BMO_Smile, 160, 128, ST7735_BLACK);
      else {
        tft.drawBitmap(0, 0, BMO_Smile, 160, 128, ST7735_BLACK);
        delay(500);
        tft.fillScreen(BMO_COLOR);
        tft.drawBitmap(0, 0, BMO_Talk, 160, 128, ST7735_BLACK);
        delay(500);
        tft.fillScreen(BMO_COLOR);
        tft.drawBitmap(0, 0, BMO_Smile, 160, 128, ST7735_BLACK);
      }
      if (millis()-LastUpdate > UpdateAudioValue){
        for(int i; i<=100; i++){
          TempAudioValue = analogRead(analogInPin);
          if (TempAudioValue > MaxAudioValue)
            MaxAudioValue = TempAudioValue;
          if (TempAudioValue < MaxAudioValue)
            MinAudioValue = TempAudioValue;
          delay(10);
        }
        FirstUpdate = false;
        LastUpdate = millis();
      }
    break;
  }  
}

void UpdateValues(int x, int y, int OldValue, int NewValue, int YRect) 
{
  tft.setTextSize(3);
  tft.fillRect(52, YRect, 60, 40, ST7735_BLACK);//para bpm -> 20; para SPO2 -> 75
  tft.setCursor(x, y);
  tft.setTextColor(ST7735_YELLOW);
  sprintf(cStringArray,"%d",NewValue);
  tft.println(cStringArray);
}

boolean handleCalibration() 
{
  int iCountDown = 10; 
  char cCountDown[15];
  boolean SensorOK = false; 
  setupCalibrateDisplay1();
  tft.setTextSize(1);
  //Start sensor Configuration
  if (!pox.begin()) {
    Serial.println("FAILED");
  } else{
    Serial.println("SUCCESS");
    SensorOK = true;
  }
  while (iCountDown > 0 && SensorOK){
    tft.setTextColor(ST7735_WHITE);
    tft.setCursor(110,100);
    sprintf(cCountDown,"%d Secs",iCountDown);
    tft.println(cCountDown);
    delay(1000);
    tft.setTextColor(ST7735_BLACK);
    tft.setCursor(110,100);
    sprintf(cCountDown,"%d Secs",iCountDown);
    tft.println(cCountDown);
    iCountDown--;
  };
  return SensorOK;
}

